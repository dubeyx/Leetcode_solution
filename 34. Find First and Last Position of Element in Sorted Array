class Solution {
    public int[] searchRange(int[] nums, int target) {
        
        int[] n= new int[2];
        n[0]=-1;
        n[1]=-1;
        int flag=0;
        for(int i=0;i<nums.length;i++)
        {
            
            if(nums[i]==target && flag==0) {n[0]=i; n[1]=i; flag=1;}
            else if(nums[i]==target) {n[1]=i;}
            
        }
        
        
        
        
        
       return n;
    }
}
