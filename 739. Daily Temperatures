class pair{
 int val;
    int index;
    pair(int val, int index)
    {
        this.val=val;
        this.index=index;
    }
    
    
    
    
    
}
class Solution {
    public int[] dailyTemperatures(int[] temp) {

        int ans[]= new int[temp.length];
        
        int check=Integer.MIN_VALUE;
        int ind=-1;
        
        for(int i=temp.length-1;i>=0;i--)
        {
        if(check<=temp[i])
        {
            check=temp[i];
            ind=i;
            
        }else
        {
            
        }
            
            
            
            
            
            
            
            
            
            
        }
        
        
        
        
        
        return ans;
        
        
    }
}
